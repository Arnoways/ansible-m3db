    4  journalctl -fu m3coordinator.service
    5  ls
    6  sysctl -w vm.max_map_count=3000000
    7  sysctl -w vm.swappiness=1
    8  systemctl restart m3coordinator.service
    9  journalctl -fu m3coordinator.service
   10  sysctl -w fs.file-max=3000000
   11  sysctl -w fs.nr_open=3000000
   12  systemctl restart m3coordinator.service
   13  journalctl -fu m3coordinator.service
   14  history
